// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model filmes {
  id        Int   @id @default(autoincrement())
     nome  String
 horario  String
  genero String
 classificacao     String
  duracao String
}

model funcionarios {
  id       Int  @id @default(autoincrement())
     nome  String
 local String
  salario String
  jornada String
}

model lanchonete {
  id        Int  @id @default(autoincrement())
    combo String
  valor String
  pipoca String
  refrigerante String
  doce String
}

